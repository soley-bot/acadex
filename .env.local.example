# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="YOUR_SUPABASE_URL"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"

# Supabase Service Role Key (NEVER expose this publicly!)
# Get this from Supabase Dashboard → Settings → API → service_role key
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY"

# Database URL (for reference)
# You can find this in Supabase Dashboard → Settings → Database → Connection string
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.your-project.supabase.co:5432/postgres"

# Site URL - IMPORTANT: Update this for production!
# Development: http://localhost:3000
# Production: https://your-domain.com
NEXT_PUBLIC_SITE_URL="http://localhost:3000"

# OpenAI API Key (if using OpenAI features)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"

# Google Gemini AI API Key (for AI enhancement features)
GOOGLE_AI_API_KEY="YOUR_GOOGLE_GEMINI_API_KEY"

# Google Sheets Import Service Account
# Get these from the JSON key file you downloaded from Google Cloud Console
# IMPORTANT: Keep the \n characters in the private key exactly as they appear
GOOGLE_SERVICE_ACCOUNT_EMAIL="your-service-account@your-project.iam.gserviceaccount.com"
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# Anthropic Claude API Key (for AI Question Enhancement)
ANTHROPIC_API_KEY="YOUR_ANTHROPIC_API_KEY"
