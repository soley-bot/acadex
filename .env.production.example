# Production Environment Variables Template
# Copy this to .env.production.local and fill in your production values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-production-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-production-service-role-key"

# Database
DATABASE_URL="postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres"

# Site URL - Use your production domain
NEXT_PUBLIC_SITE_URL="https://your-domain.com"

# API Keys (Optional - Only if using AI features)
OPENAI_API_KEY="your-production-openai-key"
GOOGLE_AI_API_KEY="your-production-google-ai-key"
ANTHROPIC_API_KEY="your-production-anthropic-key"

# Google Sheets (Optional - Only if using import features)
GOOGLE_SERVICE_ACCOUNT_EMAIL="your-service-account@your-project.iam.gserviceaccount.com"
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_KEY\n-----END PRIVATE KEY-----\n"
